{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "userAuth",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "User's unique username"
      },
      "password": {
        "type": "string",
        "description": "User's password"
      }
    },
    "required": ["username", "password"]
  }